<div class="col-md-12 col-lg-10 secao-home-cliente-row-div-central" style="text-align: center;">
  <div class="text-center my-3" style="width: 100%;">
    <h1>Pedidos</h1>
  </div>
  <table class="table table-responsive-12">
    <thead class="thead-dark">
      <tr>
        <th>Quantidade</th>
        <th>Tipo de Pagamento</th>
        <th>Valor</th>
        <th>Data</th>
        <th>Endere√ßo</th>
        <th>Status Pedidos</th>
      </tr>
    </thead>
    <tbody>
      {{#each pedidoCadastrado}}
      <tr>
        <td>{{{this.quantidadePedido}}}</td>
        <td>{{{this.tipoDePagamentoNaEntrega}}}</td>
        <td>{{{this.valorTotal}}}</td>
        <td>{{{formatDate this.createdAt}}} </td>
        <td>{{{this.Cliente.endereco}}}</td>
        <td>{{{this.statusPedidos}}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>