<div class="col-md-12 col-lg-9 secao-home-cliente-row-div-central">
    <table class="table">
        <thead class="thead-dark">
            <tr>
                <th>Quantidade</th>
                <th>valor</th>
                <th>statusPedidos</th>
            </tr>
        </thead>
        <tbody>
            {{#each pedidoCadastrado}}
            <tr>
                <td>{{{this.quantidadePedido}}}</td>
                <td>{{{this.valorTotal}}}</td>
                <td>{{{this.statusPedidos}}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>