<div class="col-md-12 col-lg-10 secao-home-cliente-row-div-central">
    <div class="text-center my-2 mb-4" style="width: 100%;">
        <h1>Estoque</h1>
    </div>
    <table class="table table-responsive-12">
        <thead class="thead-dark">
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Data</th>
                <th scope="col">Quantidade de Entrada</th>
                <th scope="col">Quantidade de Saida</th>
                <th scope="col">Quantidade em Estoque</th>
            </tr>
        </thead>
        <tbody>
            {{#each estoqueCadastrado}}
            <tr style="text-align: center;">
                <td> {{this.id}} </td>
                <td> {{formatDate this.data}} </td>
                <td> {{this.quantidadeInserida}} </td>
                <td> {{this.quantidadeVendida}} </td>
                <td> {{this.quantidadeArmazenada}} </td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>