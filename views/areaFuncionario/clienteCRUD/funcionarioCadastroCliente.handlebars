<div class="col-md-12 col-lg-10 secao-home-cliente-row-div-central">
    <form class="form" action="/dashboard/updateCliente" method="POST">
        <h2>Formulario cliente {{clienteCadastrado.id}}</h2>
        <div class="form-row">
            <input type="hidden" name="id" value="{{clienteCadastrado.id}}">
            <div class="col-md-6 mb-3">
                <label>Nome</label>
                <input type="text" class="form-control" name="nome" placeholder="nome"
                    value="{{clienteCadastrado.nome}}" required>
            </div>
            <div class="col-md-6 mb-3">
                <label>cpf</label>
                <input type="text" name="cpf" class="form-control" value="{{clienteCadastrado.cpf}}" required>
            </div>
            <div class="form-group col-md-6">
                <label>Email</label>
                <input type="email" class="form-control" name="email" placeholder="Email"
                    value="{{clienteCadastrado.email}}" required>
            </div>
            <div class="form-group col-md-6">
                <label>Senha</label>
                <input type="password" class="form-control" name="senha" placeholder="Senha" value="********" required>
            </div>
            <div class="form-group col-md-12">
                <label>Endereço</label>
                <input type="text" class="form-control" name="endereco" placeholder="Endereço"
                    value="{{clienteCadastrado.endereco}}" required>
            </div>
            <input type="submit" class="btn btn-primary" value="Atualizar">
    </form>
</div>